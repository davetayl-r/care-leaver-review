	journal = {BMJ},
	author = {Higgins, Julian P.T. and Altman, Douglas G. and Gøtzsche, Peter C. and Jüni, Peter and Moher, David and Oxman, Andrew D. and Savović, Jelena and Schulz, Kenneth F. and Weeks, Laura and Sterne, Jonathan A.C.},
	year = {2011},
	pages = {1--9},
	file = {Higgins et al. - 2011 - The Cochrane Collaboration's tool for assessing ri.pdf:/Users/davetaylor/Zotero/storage/TW9JRRQD/Higgins et al. - 2011 - The Cochrane Collaboration's tool for assessing ri.pdf:application/pdf},
